install post to posts relation ship plugin
<?php
// Stores the custom field value in Cart object
add_filter( 'woocommerce_add_cart_item_data', 'save_custom_product_field_data', 10, 2 );
function save_custom_product_field_data( $cart_item_data, $product_id ) {
     if( isset( $_REQUEST['custom_field']) ) {
        $cart_item_data[ 'custom_field' ] = $_REQUEST['custom_field'];
        // below statement make sure every add to cart action as unique line item
        $cart_item_data['unique_key'] = md5( microtime().rand() );
        WC()->session->set( 'my_order_data', $_REQUEST['custom_field'] );
    }
if( isset( $_REQUEST['hairSize']) ) {
        $cart_item_data[ 'hairSize' ] = $_REQUEST['hairSize'];
        // below statement make sure every add to cart action as unique line item
        $cart_item_data['unique_key'] = md5( microtime().rand() );
        WC()->session->set( 'my_order_data', $_REQUEST['hairSize'] );
}
     if( isset( $_REQUEST['hairLenth']) ) {
        $cart_item_data[ 'hairLenth' ] = $_REQUEST['hairLenth'];
        // below statement make sure every add to cart action as unique line item
        $cart_item_data['unique_key'] = md5( microtime().rand() );
        WC()->session->set( 'my_order_data', $_REQUEST['hairLenth'] );
    }	
	    if( isset( $_REQUEST['shop_field']) ) {
        $cart_item_data[ 'shop_field' ] = $_REQUEST['shop_field'];			
        // below statement make sure every add to cart action as unique line item
        $cart_item_data['unique_key'] = md5( microtime().rand() );
        WC()->session->set( 'my_order_data', $_REQUEST['shop_field'] );
    }	
	    if( isset( $_REQUEST['retailers_id']) ) {
        $cart_item_data[ 'retailers_id' ] = $_REQUEST['retailers_id'];			
        // below statement make sure every add to cart action as unique line item
        $cart_item_data['unique_key'] = md5( microtime().rand() );
        WC()->session->set( 'my_order_data', $_REQUEST['retailers_id'] );
    }	
    return $cart_item_data; }
add_filter( 'woocommerce_get_cart_item_from_session', function ( $cartItemData, $cartItemSessionData, $cartItemKey ) {
    if ( isset( $cartItemSessionData['shop_field'] ) ) {
        $cartItemData['shop_field'] = $cartItemSessionData['shop_field'];	
		
    }	
	    if ( isset( $cartItemSessionData['hairSize'] ) ) {
        $cartItemData['hairSize'] = $cartItemSessionData['hairSize'];
    }	
		    if ( isset( $cartItemSessionData['hairLenth'] ) ) {
        $cartItemData['hairLenth'] = $cartItemSessionData['hairLenth'];
    }	
	    if ( isset( $cartItemSessionData['custom_field'] ) ) {
        $cartItemData['custom_field'] = $cartItemSessionData['custom_field'];
    }
	    if ( isset( $cartItemSessionData['retailers_id'] ) ) {
        $cartItemData['retailers_id'] = $cartItemSessionData['retailers_id'];
    }
    return $cartItemData;
}, 10, 3 );
//To also show the data at the cart/checkout page you can use the following code:
add_filter( 'woocommerce_get_item_data', function ( $data, $cartItem ) {
    if ( isset( $cartItem['custom_field'] ) ) {
        $data[] = array(
            'name' => 'Color Selected',
            'value' => $cartItem['custom_field']
        );
    }

    return $data;
}, 10, 2 );

add_filter( 'woocommerce_get_item_data', function ( $data, $cartItem ) {
 if ( isset( $cartItem['hairSize'] ) ) {
        $data[] = array(
            'name' => 'Hair Size',
            'value' => $cartItem['hairSize']
        );
    }

    return $data;
}, 10, 2 );

add_filter( 'woocommerce_get_item_data', function ( $data, $cartItem ) {
 if ( isset( $cartItem['hairLenth'] ) ) {
        $data[] = array(
            'name' => 'Hair Length',
            'value' => $cartItem['hairLenth']
        );
    }
    return $data;
}, 10, 2 );
add_filter( 'woocommerce_get_item_data', function ( $data, $cartItemd ) {
    if ( isset( $cartItemd['shop_field'] ) ) {
        $data[] = array(
            'name' => 'Shop Selected',
            'value' => $cartItemd['shop_field']
        );
    }
    return $data;
}, 10, 2 );
add_filter( 'woocommerce_get_item_data', function ( $data, $cartItemd ) {
    if ( isset( $cartItemd['retailers_id'] ) ) {
        $data[] = array(
            'name' => 'Reatailer Id',
            'value' => $cartItemd['retailers_id']
        );
    }
    return $data;
}, 10, 2 );
add_action( 'woocommerce_add_order_item_meta', function ( $itemId, $values, $key ) {
    if ( isset( $values['custom_field'] ) ) {
        wc_add_order_item_meta( $itemId, 'Color', $values['custom_field'] );
    }	
	    if ( isset( $values['hairSize'] ) ) {
        wc_add_order_item_meta( $itemId, 'Size', $values['hairSize'] );
    }	
		    if ( isset( $values['hairLenth'] ) ) {
        wc_add_order_item_meta( $itemId, 'Length', $values['hairLenth'] );
    }		
    if ( isset( $values['shop_field'] ) ) {
        wc_add_order_item_meta( $itemId, 'Shop', $values['shop_field'] );
    }	
	    if ( isset( $values['retailers_id'] ) ) {
        wc_add_order_item_meta( $itemId, 'Ratailer', $values['retailers_id'] );
    } }, 10, 3 );
#-------------------- post to posts relation ship (show retailer name in products) -------------------------#
function my_connection_types() {
	p2p_register_connection_type( array(
		'name' => 'posts_to_pages',
		'from' => 'shop',
		'to' => 'product'
	) );
}
 add_action( 'p2p_init', 'my_connection_types' ); 
