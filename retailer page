echo "<tr><th>City  </th></td><td>-</td><td>" . $rowaCus["_billing_city"] . "</td></tr>";
echo "<tr><th>Order Total  </th></td><td>-</td><td>" . $rowaCus["order_total"] . "</td></tr>";
  } }


	 } 
 }  } else {  echo "0 results"; }

